findBtn.Activated:Connect(function()
    findRemote()
end)

hookBtn.Activated:Connect(function()
    hookFireClient()
end)

testBtn.Activated:Connect(function()
    testOnClientEvent()
end)

sendBatchBtn.Activated:Connect(function()
    if #pendingEvents > 0 then
        addLog("\n=== Manually sending batch ===")
        sendBatchWebhook(pendingEvents)
        addLog("Sent " .. #pendingEvents .. " events")
        pendingEvents = {}
    else
        addLog("No events to send!")
    end
end)

-- Auto-find on load
task.wait(1)
addLog("HTTP Library: " .. (httpLibName ~= "" and httpLibName or "NOT FOUND"))
findRemote()

