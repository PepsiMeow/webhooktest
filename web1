findBtn.Activated:Connect(function()
    findRemote()
end)

hookBtn.Activated:Connect(function()
    hookFireClient()
end)

testBtn.Activated:Connect(function()
    testOnClientEvent()
end)

sendBatchBtn.Activated:Connect(function()
    addLog("\n=== Manual Send Batch ===")
    addLog("Hook count: " .. hookCount)
    addLog("pendingEvents type: " .. type(pendingEvents))
    addLog("pendingEvents count: " .. #pendingEvents)
    
    -- Check if pendingEvents is actually the global one
    if pendingEvents == nil then
        addLog("ERROR: pendingEvents is nil!")
        return
    end
    
    -- Make sure we're using the global pendingEvents - copy all events
    local eventsToSend = {}
    local count = 0
    for i, event in ipairs(pendingEvents) do
        count = count + 1
        table.insert(eventsToSend, event)
    end
    
    addLog("Copied " .. count .. " events to send")
    
    if #eventsToSend > 0 then
        addLog("Sending " .. #eventsToSend .. " events to webhook...")
        sendBatchWebhook(eventsToSend)
        -- Clear pending events
        pendingEvents = {}
        if statusLabel then
            statusLabel.Text = "Events: " .. hookCount .. " | Pending: 0/" .. BATCH_SIZE .. " (Sent!)"
        end
        addLog("Batch sent and cleared!")
    else
        addLog("ERROR: No events to send!")
        addLog("This might mean events aren't being stored properly.")
        addLog("Try hatching an egg first, then check if pendingEvents updates.")
    end
end)

closeBtn.Activated:Connect(function()
    addLog("Closing script...")
    -- Unhook if hooked
    if originalFireClient and remoteEvent then
        pcall(function()
            remoteEvent.FireClient = originalFireClient
        end)
        addLog("Unhooked FireClient")
    end
    -- Send any pending events before closing
    if #pendingEvents > 0 then
        addLog("Sending final batch before close...")
        local finalBatch = {}
        for i = 1, #pendingEvents do
            table.insert(finalBatch, pendingEvents[i])
        end
        sendBatchWebhook(finalBatch)
        task.wait(1) -- Wait for webhook to send
    end
    -- Close GUI
    pcall(function()
        g:Destroy()
    end)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Remote Hook Test",
        Text = "Script closed",
        Duration = 3
    })
    -- Stop script execution
    error("Script closed by user")
end)

-- Auto-find on load
task.wait(1)
addLog("HTTP Library: " .. (httpLibName ~= "" and httpLibName or "NOT FOUND"))
findRemote()

